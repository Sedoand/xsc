        $('#posts ul').append(html);
        $('#loading').hide();
      }
    });
  }
});*/
});
/*-------- Load more End ----------*/
/*-------- Grids Start ----------*/
$(function() {
        $(".free-wall").each(function() {
          var wall = new Freewall(this);
          wall.reset({
            selector: '.thumb-b',
            animate: true,
            cellW: 185,
            cellH: 278,
            fixSize: 0,
            gutterY: 0,
            gutterX: -15,
            onResize: function() {
              wall.fitWidth();
            }
          })
          wall.fitWidth();
        });
        $(window).trigger("resize");
      });
/*-------- Grids End ----------*/



